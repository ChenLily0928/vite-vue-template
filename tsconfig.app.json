{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": ["src/types/env.d.ts", "src/**/*", "src/**/*.vue"],
    "exclude": ["src/**/__tests__/*"],
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@assets/*": ["./src/assets/*"],
            "@components/*": ["./src/components/*"],
            "@configs/*": ["./src/configs/*"],
            "@constants/*": ["./src/constants/*"],
            "@mocks/*": ["./src/mocks/*"],
            "@pages/*": ["./src/pages/*"],
            "@router/*": ["./src/router/*"],
            "@store/*": ["./src/store/*"],
            "@styles/*": ["./src/styles/*"],
            "@types/*": ["./src/types/*"],
            "@utils/*": ["./src/utils/*"]
        }
    }
}
